<ibatorConfiguration>
  <ibatorContext id="FlatJava5" targetRuntime="Ibatis2Java5" defaultModelType="flat">
    <ibatorPlugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
    <ibatorPlugin type="org.mybatis.generator.plugins.SqlMapConfigPlugin" >
      <property name="targetPackage" value="com.boeing.it.eis.generated" />
      <property name="targetProject" value="MAVEN" />
    </ibatorPlugin>

    <jdbcConnection driverClass="org.postgresql.Driver"
        connectionURL="jdbc:postgresql://localhost:5432/authority_admin"
        userId="postgres" password="pass"/>

    <javaModelGenerator targetPackage="com.boeing.it.eis.generated.model" targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.boeing.it.eis.generated.xml"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <daoGenerator type="GENERIC-CI" targetPackage="com.boeing.it.eis.generated.dao"  targetProject="MAVEN">
      <property name="enableSubPackages" value="true" />
    </daoGenerator>

