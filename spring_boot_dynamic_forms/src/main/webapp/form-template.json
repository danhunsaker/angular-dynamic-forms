{
   "fuelload": {
      "type": "fieldset",
      "label": "Fuel Load",
      "fields": {
         "fuelloadopt" : {
            "label" : "Fuel Load Options",
            "options" : {
               "FuelLoadGroup" : {
                  "label" : "Fuel Load Group"
               },
               "Optional" : {
                  "label" : "Optional"
               }
            },
            "type" : "select"
         },         
         "fuelloadgroup": {
            "type": "fieldset",
            "label": "Fuel Load Group",
            "attributes": {"ng-disabled": "urlFormData.fuelloadopt != 'FuelLoadGroup'"},
            "fields": {   
		         "maingroup": {
		            "type": "fieldset",
		            "label": "Main",
		            "fields": {   
		               "mainoptions": {
		                  "type" : "select",
		                  "label": "Main Options",
		                  "options": {
		                     "mLbsOpt" : {
		                        "label" : "Lbspertank"
		                     },
		                     "mPercentageOpt" : {
		                        "label" : "Percentage"
		                     }
		                  }
		               },
                       "lbspertankgroup": {
                          "type": "fieldset",
                          "label": "Lbspertank Group",
                          "attributes": {"ng-disabled": "urlFormData.mainoptions != 'mLbsOpt'"},
                          "fields": {   
		                      "Lbspertank": {
		                          "type": "number",
		                          "minValue": 200,
		                          "maxValue": 300
    		                  }
    		              }
                       },
                       "mpercentagegroup": {
                          "type": "fieldset",
                          "label": "Percentage Group",
                          "attributes": {"ng-disabled": "urlFormData.mainoptions != 'mPercentageOpt'"},
                          "fields": {   
		                     "CPercentage": {
		                        "type": "number",
		                        "minValue": 0,
		                        "maxValue": 100
		                     }
		                  }
		               }
		            }
		         },
		         "centergroup": {
		            "type": "fieldset",
		            "label": "Center",
		            "fields": {   
		               "centeroptions": {
		                  "type" : "select",
		                  "label": "Center Options",
		                  "options": {
		                     "CLbsOpt" : {
		                        "label" : "Lbs"
		                     },
		                     "CPercentageOpt" : {
		                        "label" : "Percentage"
		                     }
		                  }
		               },
                       "clbsgroup": {
                          "type": "fieldset",
                          "label": "Lbs Group",
                          "attributes": {"ng-disabled": "urlFormData.centeroptions != 'CLbsOpt'"},
                          "fields": {   
                             "Lbs": {
		                        "type": "number",
		                        "label": "Lbs",
		                        "minValue": 200,
		                        "maxValue": 300
		                     }
		                  }
		               },
                       "cpercentagegroup": {
                          "type": "fieldset",
                          "label": "Percentage Group",
                          "attributes": {"ng-disabled": "urlFormData.centeroptions != 'CPercentageOpt'"},
                          "fields": {   
		                     "CPercentage": {
		                        "type": "number",
		                        "label": "Percentage",
		                        "minValue": 0,
		                        "maxValue": 100
		                     }
		                  }
		               }
		            }
		         },
		         "othersgroup": {
                    "type": "fieldset",
                    "label": "Others",
                    "name": "Others",
                       "fields": {   
                       "txtOthers": {
                          "type": "textarea",
                          "label": "Others",
                          "name": "txtOthers"
                    }
                 }
            }
         }
     },
     "Optional": {
         "type": "label",
         "label": "Optional"
      },
      "submit": {
         "type": "submit",
         "label": "Submit"
      }
    }
  }
}
